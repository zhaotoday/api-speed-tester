{
  "name": "api-speed-tester",
  "version": "0.0.2",
  "description": "一个用于测试多个 API 域名响应速度的 TypeScript 工具，帮助选择最快的 API 线路",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "tsx src/example.ts",
    "example": "tsx src/example.ts"
  },
  "keywords": [
    "api",
    "speed",
    "test",
    "route",
    "performance",
    "benchmark",
    "network",
    "latency",
    "response-time",
    "typescript",
    "axios",
    "cdn",
    "load-balancer"
  ],
  "author": {
    "name": "CodeBuddy",
    "email": "codebuddy@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codebuddy/api-speed-tester.git"
  },
  "bugs": {
    "url": "https://github.com/codebuddy/api-speed-tester/issues"
  },
  "homepage": "https://github.com/codebuddy/api-speed-tester#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}